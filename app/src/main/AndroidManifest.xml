<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.god.magic">
   <uses-permission android:name="android.permission.INTERNET"/>
    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />
    <application
        android:name=".FirebaseDB"
        android:allowBackup="true"
        android:icon="@mipmap/yzz_72"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MagicActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ToDoMoreInfo">
            <intent-filter>
                <action android:name="android.intent.action.INFO" />

                <category android:name="android.intent.category.EDIT" />
            </intent-filter>
        </activity>
        <activity android:name=".SharedPreferences">
            <intent-filter>
                <action android:name="android.intent.action.SHARED_PREF" />

                <category android:name="android.intent.category.EDIT" />
            </intent-filter>
        </activity>
    </application>

</manifest>